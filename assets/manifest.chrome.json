{
	"name": "Live Sass.js Editor",
	"short_name" : "Sass.js",
	"version": "0.1.17",
	"manifest_version": 2,
	"description": "Offline Playground for Sass",
	"permissions": ["activeTab"],
	"browser_action": {
		"default_title": "Start Sass.js Edit"
	},
		"background": {
		"scripts": ["main.js"],
		"persistent": false
	},
		"icons": {
			"16": "sass-16.png",
			"48": "sass-48.png",
			"128": "sass-128.png"
	}
	

}
